#include <stdio.h>

struct Employee{
	char name[30];
	int id;
};

int main(){
	struct Employee employee1, employee2 , employee3;
	employee1.name = strcpy("Ahmad"); employee1.id = 20;
	employee2.name = strcpy("Amr"); employee1.id = 21;
	employee2.name = strcpy("Maher"); employee1.id = 22;



	return 0;
}
