#include <stdio.h>

int main(){
	char input;
	printf("Enter a character: ");
	fflush(stdin); fflush(stdout);
	scanf("%c", &input);

	/*
	char a = 'a', z = 'z' ,A = 'A', Z ='Z';
	printf("a: %d\n" , a);
	printf("z: %d\n" , z);
	printf("A: %d\n" , A);
	printf("Z: %d" , Z);
	*/
	if ((input >= 'a' && input <= 'z') ||(input >= 'A' && input <= 'Z')){
		printf ("%c is an alphabet", input);
	}
	else printf ("%c is not an alphabet", input);

	return 0;
}
