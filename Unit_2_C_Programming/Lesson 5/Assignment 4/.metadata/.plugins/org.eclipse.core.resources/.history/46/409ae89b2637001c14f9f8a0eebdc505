#include "stdio.h"

long calculate_factorial(int n);

int main(){
	int n;
	printf("Enter a positive integer: ");
	fflush(stdin); fflush(stdout);
	scanf("%d", &n);
	printf("Factorial of %d = %ld", n, calculate_factorial(n));
	return 0;
}

long calculate_factorial(int n){
	return n * calculate_factorial(n - 1);
}
