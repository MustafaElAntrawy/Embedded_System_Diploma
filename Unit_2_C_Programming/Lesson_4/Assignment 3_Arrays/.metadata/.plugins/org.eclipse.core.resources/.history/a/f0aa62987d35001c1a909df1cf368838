#include <stdio.h>

int main (){
	int arr[100];
	int n, element;

	printf("Enter number of elements: ");
	fflush(stdin); fflush(stdout);
	scanf("%d", &n);

	int i, j;
	for (i = 0; i < n; ++i){
		arr[i] = i + 1;
		printf("%d ", arr[i]);
	}
	printf("\nEnter the elements to be inserted: ");
	fflush(stdin); fflush(stdout);
	scanf("%d", &element);

	for (i = 0, j = 0; i < n; ++i, ++j){
		if( element == arr[i]){

			break;
		}
	}

	return 0;
}
